@charset "utf-8";
.body{
	margin:0 auto;
	width:1000px;
	height:1500px;
}
/*banner部分开始*/
.head{
    width:800px;
	height:120px;
	margin-left:100px;
	background:url(file:///D|/phpStudy/WWW/tsgl/moban/images/book_02.gif);
	position:relative;
}
	.login{
		width:100px;	
		text-align:center;
		margin-left:680px;
		margin-top:20px;
		position:absolute;
	}
	.login a{
		text-decoration:none;
		font-size:15px;
	
	}
	.login a:hover{
		color:#F00;
	}
	.nav{
		width:800px;
		height:30px;
		position:relative;
	}
		.nav-left{
			width:400px;
			height:30px;
			position:relative;
			left:100px;
			line-height:30px;
			
		}
		.nav-right{
			width:200px;
			height:30px;
			position:absolute;
			line-height:30px;
			left:780px;
			top:-15px;
			
		}
		.nav-left a{
			width:100px;
			display:inline-block;
			height:30px;
			text-align:center;
			text-decoration:none;
				
		}
		.nav-left a:hover{
			width:100px;
			height:30px;
			display:inline-block;
			background:#0F0;
			text-align:center;
		}
/*banner部分结束*/

